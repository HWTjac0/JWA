<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
    fx:controller="com.example.jaqweatherapp.ChartController"
>
    <padding>
        <Insets left="10" right="10" top="10" bottom="10" />
    </padding>
    <VBox>
        <HBox>
            <VBox spacing="5">
                <Label styleClass="filterChartLabel">Wybierz filtr:</Label>
                <VBox fx:id="filterList" spacing="10" styleClass="filterChartList">
                    <padding>
                        <Insets left="10" right="10" top="10" bottom="10"/>
                    </padding>
                </VBox>
            </VBox>
            <VBox></VBox>
        </HBox>
        <HBox></HBox>
    </VBox>
    <HBox fx:id="chartContainer" VBox.vgrow="ALWAYS">

    </HBox>
</VBox>
